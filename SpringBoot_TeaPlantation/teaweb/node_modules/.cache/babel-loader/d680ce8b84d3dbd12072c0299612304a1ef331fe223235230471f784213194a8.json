{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"repositoryManageMain\",\n  data() {\n    return {\n      user: JSON.parse(sessionStorage.getItem(\"CurUser\")),\n      tableData: [],\n      rid: null,\n      eid: null,\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      actionForm: {\n        actorid: null,\n        power: '',\n        action: '',\n        actiontime: '',\n        status: ''\n      }\n    };\n  },\n  methods: {\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n      this.pageNum = 1;\n      this.pageSize = val;\n      this.loadPost();\n    },\n    handleCurrentChange(val) {\n      console.log(`当前页: ${val}`);\n      this.pageNum = val;\n      this.loadPost();\n    },\n    resetForm() {\n      this.actionForm.actorid = null;\n      this.actionForm.power = '';\n      this.actionForm.action = '';\n      this.actionForm.actiontime = '';\n      this.actionForm.status = '';\n      this.actionForm.eid = null;\n    },\n    loadPost() {\n      if (this.user != null) {\n        this.$axios.post(this.$httpUrl + '/equipment/listAllRepositoryPage', {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          param: {\n            rid: this.rid,\n            eid: this.eid\n          }\n        }).then(res => res.data).then(res => {\n          console.log(res);\n          if (res.code == 200) {\n            this.tableData = res.data;\n            this.total = res.total;\n          } else {\n            alert(\"数据获取失败\");\n          }\n        });\n      } else {\n        this.$confirm('未登入请登录后再进入？', '提示', {\n          confirmButtonText: '确定去登入',\n          //确认按钮文字显示\n          type: 'warning',\n          center: true,\n          //文字居中\n          showCancelButton: false,\n          //不显示取消按钮\n          closeOnClickModal: false //是否可以点击空白处关闭弹窗\n        }).then(() => {\n          this.$router.push('/');\n          sessionStorage.clear();\n        }).catch(() => {\n          this.$message({\n            type: \"info\",\n            message: \"大哥哥(姐姐)快去登录吧！！\"\n          });\n        });\n      }\n    },\n    resetParam() {\n      this.rid = null;\n      this.eid = null;\n    },\n    approvalPush(row) {\n      //给行为actionForm赋值\n      this.actionForm.actorid = this.user.uid;\n      this.actionForm.power = this.user.role;\n      this.actionForm.action = '出库审批';\n      this.actionForm.actiontime = new Date();\n      this.actionForm.status = 'resolve';\n      this.actionForm.eid = row.eid;\n      this.toApproval();\n      //重置\n      this.$nextTick(() => {\n        this.resetForm();\n      });\n    },\n    approvalPop(row) {\n      //给行为actionForm赋值\n      this.actionForm.actorid = this.user.uid;\n      this.actionForm.power = this.user.role;\n      this.actionForm.action = '入库审批';\n      this.actionForm.actiontime = new Date();\n      this.actionForm.status = 'resolve';\n      this.actionForm.eid = row.eid;\n      this.toApproval();\n      //重置\n      this.$nextTick(() => {\n        this.resetForm();\n      });\n    },\n    toApproval() {\n      this.$axios.post(this.$httpUrl + '/action/addAction', this.actionForm).then(res => res.data).then(res => {\n        if (res.code == 200) {\n          this.$message({\n            showClose: true,\n            message: '数据添加成功',\n            type: 'success'\n          });\n          //跳转到提交页面\n          this.$router.push('/approvalMain');\n        } else {\n          this.$message({\n            showClose: true,\n            message: '数据添加失败',\n            type: 'error'\n          });\n        }\n      });\n    }\n  },\n  beforeMount() {\n    this.loadPost();\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","sessionStorage","getItem","tableData","rid","eid","pageNum","pageSize","total","actionForm","actorid","power","action","actiontime","status","methods","handleSizeChange","val","console","log","loadPost","handleCurrentChange","resetForm","$axios","post","$httpUrl","param","then","res","code","alert","$confirm","confirmButtonText","type","center","showCancelButton","closeOnClickModal","$router","push","clear","catch","$message","message","resetParam","approvalPush","row","uid","role","Date","toApproval","$nextTick","approvalPop","showClose","beforeMount"],"sources":["src/components/equipment/repositoryManageMain.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-bottom: 5px\">\r\n      <el-input v-model=\"rid\"\r\n                placeholder=\"请输入仓库id\"\r\n                suffix-icon=\"el-icon-search\"\r\n                style=\"width: 200px\"\r\n                @keyup.enter.native=\"loadPost\"></el-input>\r\n      <el-input v-model=\"eid\"\r\n                placeholder=\"请输入设备id\"\r\n                suffix-icon=\"el-icon-search\"\r\n                style=\"width: 200px\"\r\n                @keyup.enter.native=\"loadPost\"></el-input>\r\n      <el-button type=\"primary\"\r\n                 style=\"margin-left: 5px\"\r\n                 @click=\"loadPost\">查询</el-button>\r\n      <el-button type=\"success\"\r\n                 @click=\"resetParam\">重置</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\"\r\n              :header-cell-style=\"{backgroundColor:'rgba(156,180,184,0.5)'}\"\r\n              :border=\"true\">\r\n      <el-table-column prop=\"rid\" label=\"仓库Id\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"rname\" label=\"仓库名称\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"eid\" label=\"设备Id\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"stock\" label=\"库存\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"pushnumber\" label=\"需要入库是数量\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"popnumber\" label=\"需要出库的数量\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"operate\" label=\"操作\">\r\n        <template slot-scope=\"scop