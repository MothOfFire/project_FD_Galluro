\n      });\r\n    },\r\n    toApproval() {\r\n      this.$axios.post(this.$httpUrl+'/action/addAction', this.actionForm)\r\n          .then(res=>res.data).then(res=>{\r\n        if(res.code==200){\r\n          this.$message({\r\n            showClose: true,\r\n            message: '数据添加成功',\r\n            type: 'success'\r\n          });\r\n          //跳转到提交页面\r\n          this.$router.push('/approvalMain');\r\n        }else {\r\n          this.$message({\r\n            showClose: true,\r\n            message: '数据添加失败',\r\n            type: 'error'\r\n          });\r\n        }\r\n      });\r\n    }\r\n  },\r\n  beforeMount() {\r\n  this.loadPost();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AA4DA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,OAAA;MACAC,SAAA;MACAC,GAAA;MACAC,GAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,UAAA;QACAC,OAAA;QACAC,KAAA;QACAC,MAAA;QACAC,UAAA;QACAC,MAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,iBAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,OAAAF,GAAA;MACA,KAAAX,OAAA;MACA,KAAAC,QAAA,GAAAU,GAAA;MACA,KAAAG,QAAA;IACA;IACAC,oBAAAJ,GAAA;MACAC,OAAA,CAAAC,GAAA,SAAAF,GAAA;MACA,KAAAX,OAAA,GAAAW,GAAA;MACA,KAAAG,QAAA;IACA;IACAE,UAAA;MACA,KAAAb,UAAA,CAAAC,OAAA;MACA,KAAAD,UAAA,CAAAE,KAAA;MACA,KAAAF,UAAA,CAAAG,MAAA;MACA,KAAAH,UAAA,CAAAI,UAAA;MACA,KAAAJ,UAAA,CAAAK,MAAA;MACA,KAAAL,UAAA,CAAAJ,GAAA;IACA;IACAe,SAAA;MACA,SAAAtB,IAAA;QACA,KAAAyB,MAAA,CAAAC,IAAA,MAAAC,QAAA,uCACA;UACAnB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAmB,KAAA;YACAtB,GAAA,OAAAA,GAAA;YACAC,GAAA,OAAAA;UACA;QACA,EACA,CAAAsB,IAAA,CAAAC,GAAA,IAAAA,GAAA,CAAA/B,IAAA,EAAA8B,IAAA,CAAAC,GAAA;UACAV,OAAA,CAAAC,GAAA,CAAAS,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YACA,KAAA1B,SAAA,GAAAyB,GAAA,CAAA/B,IAAA;YACA,KAAAW,KAAA,GAAAoB,GAAA,CAAApB,KAAA;UACA;YACAsB,KAAA;UACA;QACA;MACA;QACA,KAAAC,QAAA;UACAC,iBAAA;UAAA;UACAC,IAAA;UACAC,MAAA;UAAA;UACAC,gBAAA;UAAA;UACAC,iBAAA;QACA,GAAAT,IAAA;UACA,KAAAU,OAAA,CAAAC,IAAA;UACArC,cAAA,CAAAsC,KAAA;QACA,GAAAC,KAAA;UACA,KAAAC,QAAA;YACAR,IAAA