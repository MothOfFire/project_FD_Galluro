{"ast":null,"code":"export default {\n  name: \"teaPluckMain\",\n  data() {\n    return {\n      user: JSON.parse(sessionStorage.getItem(\"CurUser\")),\n      tableData: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      speed: null,\n      gathertime: '',\n      times: [{\n        value: '春季',\n        label: '春季'\n      }, {\n        value: '秋季',\n        label: '秋季'\n      }],\n      centerDialogVisible: false,\n      form: {\n        gatherid: null,\n        gathertime: '',\n        status: '',\n        makekind: '',\n        speed: 0,\n        number: null,\n        isbegin: 'false',\n        isend: 'false',\n        kind: ''\n      },\n      rules: {\n        makekind: [{\n          required: true,\n          message: '请输入制作的成品茶叶种类',\n          trigger: 'blur'\n        }, {\n          min: 1,\n          max: 3,\n          message: '长度在 1 到 3 个字符',\n          trigger: 'blur'\n        }],\n        number: [{\n          required: true,\n          message: '请输入人数',\n          trigger: 'blur'\n        }, {\n          min: 1,\n          max: 3,\n          message: '长度在 1 到 3 个字符',\n          trigger: 'blur'\n        }, {\n          pattern: /^([1-9][0-9]*){1,3}$/,\n          message: '人数必须是正整数',\n          trigger: 'blur'\n        }],\n        kind: [{\n          required: true,\n          message: '请输入茶树种类',\n          trigger: 'blur'\n        }, {\n          min: 1,\n          max: 7,\n          message: '长度在 1 到 7 个字符',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {},\n  beforeMount() {}\n};","map":{"version":3,"names":["name","data","user","JSON","parse","sessionStorage","getItem","tableData","pageNum","pageSize","total","speed","gathertime","times","value","label","centerDialogVisible","form","gatherid","status","makekind","number","isbegin","isend","kind","rules","required","message","trigger","min","max","pattern","methods","beforeMount"],"sources":["src/components/teaLeaf/teaPluckMain.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-bottom: 5px\">\r\n      <el-input v-model=\"speed\"\r\n                placeholder=\"请输入进度\"\r\n                suffix-icon=\"el-icon-search\"\r\n                style=\"width: 200px\"></el-input>\r\n      <el-select v-model=\"gathertime\"\r\n                 filterable placeholder=\"请选择采摘时间\"\r\n                 style=\"margin-left: 5px\">\r\n        <el-option\r\n            v-for=\"item in times\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\">\r\n        </el-option>\r\n      </el-select>\r\n      <el-button type=\"primary\"\r\n                 style=\"margin-left: 5px\"\r\n                 @click=\"loadPost\">查询</el-button>\r\n      <el-button type=\"success\"\r\n                 @click=\"resetParam\">重置</el-button>\r\n      <el-button type=\"primary\"\r\n                 style=\"margin-left: 5px\"\r\n                 @click=\"add\">新增</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\"\r\n              :header-cell-style=\"{backgroundColor:'rgba(156,180,184,0.5)'}\"\r\n              :border=\"true\">\r\n      <el-table-column prop=\"gatherid\" label=\"gatherId\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"makekind\" label=\"制作茶叶种类\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"kind\" label=\"茶树种类\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"status\" label=\"茶叶状态\" width=\"160\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"gathertime\" label=\"采摘季节\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag\r\n              :type=\"scope.row.gathertime === '春季' ? 'primary' : 'success'\"\r\n              disable-transitions>{{scope.row.gathertime === '春季' ? '春季' : '秋季'}}</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"speed\" label=\"进度\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"number\" label=\"采摘人数\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"isbegin\" label=\"是否已经开始\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag\r\n              :type=\"scope.row.isbegin === 'true' ? 'primary' : 'success'\"\r\n              disable-transitions>{{scope.row.isbegin === 'true' ? '是' : '不是'}}</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"isend\" label=\"是否已经结束\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag\r\n              :type=\"scope.row.isend === 'true' ? 'primary' : 'success'\"\r\n              disable-transitions>{{scope.row.isend === 'true' ? '是' : '不是'}}</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"operate\" label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button size=\"small\"\r\n                     type=\"success\"\r\n                     @click=\"updateGather(scope.row)\">修改</el-button>\r\n          <el-popconfirm title=\"确定删除吗？\"\r\n                         style=\"margin-left: 5px\"\r\n                         @confirm=\"deleteGather(scope.row)\">\r\n            <el-button slot=\"reference\"\r\n                       size=\"small\"\r\n                       type=\"danger\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!--分页组件-->\r\n    <el-pagination\r\n        style=\"margin-top: 15px\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"pageNum\"\r\n        :page-sizes=\"[ 5, 10, 15,20]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n    </el-pagination>\r\n    <!--对话框 centerDialogVisibley用来控制对话框是否出现（true表示出现）-->\r\n    <!--超级管理员对话框-->\r\n    <el-dialog\r\n        title=\"提示\"\r\n        :visible.sync=\"centerDialogVisible\"\r\n        width=\"30%\"\r\n        center\r\n        :before-close=\"handleClose\">\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item prop=\"status\" label=\"状态\">\r\n          <el-radio-group v-model=\"form.status\">\r\n            <el-radio label=\"初芽\">初芽</el-radio>\r\n            <el-radio label=\"嫩芽\">嫩芽</el-radio>\r\n            <el-radio label=\"嫩叶\">嫩叶</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item prop=\"gathertime\" label=\"采摘季节\">\r\n          <el-radio-group v-model=\"form.gathertime\">\r\n            <el-radio label=\"春季\">春季</el-radio>\r\n            <el-radio label=\"秋季\">秋季</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item prop=\"makekind\" label=\"制作种类\">\r\n          <el-input v-model=\"form.makekind\" style=\"width: 80%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"number\" label=\"采摘人数\">\r\n          <el-input v-model=\"form.number\" style=\"width: 80%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"isbegin\" label=\"是否开始\">\r\n          <el-radio-group v-model=\"form.isbegin\">\r\n            <el-radio label=\"true\">已经开始</el-radio>\r\n            <el-radio label=\"false\">还没开始</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item prop=\"isend\" label=\"是否结束\">\r\n          <el-radio-group v-model=\"form.isbegin\">\r\n            <el-radio label=\"true\">已经结束</el-radio>\r\n            <el-radio label=\"false\">还没结束</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item prop=\"kind\" label=\"茶树种类\">\r\n          <el-input v-model=\"form.kind\" style=\"width: 80%\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n    <el-button @click=\"centerDialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n  </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"teaPluckMain\",\r\n  data() {\r\n    return {\r\n      user : JSON.parse(sessionStorage.getItem(\"CurUser\")),\r\n      tableData:[],\r\n      pageNum:1,\r\n      pageSize:10,\r\n      total:0,\r\n      speed: null,\r\n      gathertime:'',\r\n      times:[\r\n        {\r\n          value: '春季',\r\n          label: '春季'\r\n        }, {\r\n          value: '秋季',\r\n          label: '秋季'\r\n        }\r\n      ],\r\n      centerDialogVisible:false,\r\n      form:{\r\n        gatherid: null,\r\n        gathertime: '',\r\n        status: '',\r\n        makekind: '',\r\n        speed: 0,\r\n        number: null,\r\n        isbegin: 'false',\r\n        isend: 'false',\r\n        kind: ''\r\n      },\r\n      rules: {\r\n        makekind:[\r\n          { required: true, message: '请输入制作的成品茶叶种类', trigger: 'blur' },\r\n          { min: 1, max: 3, message: '长度在 1 到 3 个字符', trigger: 'blur' },\r\n        ],\r\n        number: [\r\n          { required: true, message: '请输入人数', trigger: 'blur' },\r\n          { min: 1, max: 3, message: '长度在 1 到 3 个字符', trigger: 'blur' },\r\n          {pattern: /^([1-9][0-9]*){1,3}$/,message: '人数必须是正整数',trigger: 'blur'},\r\n        ],\r\n        kind: [\r\n          { required: true, message: '请输入茶树种类', trigger: 'blur' },\r\n          { min: 1, max: 7, message: '长度在 1 到 7 个字符', trigger: 'blur' },\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n\r\n  },\r\n  beforeMount() {\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA6IA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,OAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,KAAA;MACAC,UAAA;MACAC,KAAA,GACA;QACAC,KAAA;QACAC,KAAA;MACA;QACAD,KAAA;QACAC,KAAA;MACA,EACA;MACAC,mBAAA;MACAC,IAAA;QACAC,QAAA;QACAN