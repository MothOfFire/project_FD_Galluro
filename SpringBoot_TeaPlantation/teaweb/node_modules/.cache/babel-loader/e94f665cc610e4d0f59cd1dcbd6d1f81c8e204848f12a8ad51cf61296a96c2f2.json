{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"teaPantationParkMain\",\n  data() {\n    return {\n      user: JSON.parse(sessionStorage.getItem(\"CurUser\")),\n      tableData: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      pid: '',\n      centerDialogVisible: false,\n      form: {\n        pname: '',\n        area: '',\n        aid: '',\n        pFunction: ''\n      },\n      rules: {\n        pname: [{\n          required: true,\n          message: '请输入名称',\n          trigger: 'blur'\n        }],\n        area: [{\n          required: true,\n          message: '请输入面积',\n          trigger: 'blur'\n        }],\n        aid: [{\n          required: true,\n          message: '请输入管理员ID',\n          trigger: 'blur'\n        }],\n        pFunction: [{\n          required: true,\n          message: '请输入园区职能',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n      this.pageNum = 1;\n      this.pageSize = val;\n      this.loadPost();\n    },\n    handleCurrentChange(val) {\n      console.log(`当前页: ${val}`);\n      this.pageNum = val;\n      this.loadPost();\n    },\n    loadPost() {\n      if (this.user != null && (this.user.role == '0' || this.user.role == '1')) {\n        this.$axios.post(this.$httpUrl + '/teaPark/listAllPage', {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          param: {\n            pid: this.pid\n          }\n        }).then(res => res.data).then(res => {\n          console.log(res);\n          if (res.code == 200) {\n            this.tableData = res.data;\n            this.total = res.total;\n          } else {\n            alert(\"数据获取失败\");\n          }\n        });\n      } else {\n        this.$confirm('未登入请登录后再进入？', '提示', {\n          confirmButtonText: '确定去登入',\n          //确认按钮文字显示\n          type: 'warning',\n          center: true,\n          //文字居中\n          showCancelButton: false,\n          //不显示取消按钮\n          closeOnClickModal: false //是否可以点击空白处关闭弹窗\n        }).then(() => {\n          this.$router.push('/');\n          sessionStorage.clear();\n        }).catch(() => {\n          this.$message({\n            type: \"info\",\n            message: \"大哥哥(姐姐)快去登录吧！！\"\n          });\n        });\n      }\n    },\n    resetParam() {\n      this.pid = '';\n    },\n    add() {\n      this.centerDialogVisible = true;\n      this.$nextTick(() => {\n        this.resetForm();\n      });\n    },\n    updatePark(row) {\n      console.log(row);\n      this.centerDialogVisible = true;\n      this.$nextTick(() => {\n        //赋值到表单\n        this.form.pid = row.pid;\n        this.form.pname = row.pname;\n        this.form.aid = row.aid;\n        this.form.area = row.area + '';\n        this.form.pFunction = row.function;\n      });\n    },\n    deletePark(row) {\n      console.log(row);\n      this.$axios.get(this.$httpUrl + '/teaPark/deletePark?pid=' + row.pid).then(res => res.data).then(res => {\n        console.log(res);\n        if (res.code == 200) {\n          this.$message({\n            showClose: true,\n            message: '数据删除成功',\n            type: 'success'\n          });\n          this.loadPost();\n        } else {\n          this.$message({\n            showClose: true,\n            message: '数据删除失败',\n            type: 'error'\n          });\n        }\n      });\n    },\n    handleClose(done) {\n      this.$confirm('确认关闭？').then(_ => {\n        done();\n      }).catch(_ => {});\n    },\n    save() {\n      this.$refs.form.validate(valid => {\n        if (valid) {\n          if (this.form.uid) {\n            this.doUpdate();\n          } else {\n            this.doSave();\n          }\n        } else {\n          console.log('Error!!!');\n          return false;\n        }\n      });\n    },\n    doSave() {\n      this.$axios.post(this.$httpUrl + '/teaPark/addPark', this.form).then(res => res.data).then(res => {\n        console.log(res);\n        if (res.code == 200) {\n          this.$message({\n            showClose: true,\n            message: '数据添加成功',\n            type: 'success'\n          });\n          this.centerDialogVisible = false;\n          this.loadPost();\n          this.resetForm();\n        } else {\n          this.$message({\n            showClose: true,\n            message: '数据添加失败',\n            type: 'error'\n          });\n        }\n      });\n    },\n    doUpdate() {\n      this.$axios.post(this.$httpUrl + '/teaPark/updatePark', this.form).then(res => res.data).then(res => {\n        console.log(res);\n        if (res.code == 200) {\n          this.$message({\n            showClose: true,\n            message: '数据修改成功',\n            type: 'success'\n          });\n          this.centerDialogVisible = false;\n          this.loadPost();\n          this.resetForm();\n        } else {\n          this.$message({\n            showClose: true,\n            message: '数据修改失败',\n            type: 'error'\n          });\n        }\n      });\n    },\n    resetForm() {\n      this.$refs.form.resetFields();\n    }\n  },\n  beforeMount() {\n    this.loadPost();\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","sessionStorage","getItem","tableData","pageNum","pageSize","total","pid","centerDialogVisible","form","pname","area","aid","pFunction","rules","required","message","trigger","methods","handleSizeChange","val","console","log","loadPost","handleCurrentChange","role","$axios","post","$httpUrl","param","then","res","code","alert","$confirm","confirmButtonText","type","center","showCancelButton","closeOnClickModal","$router","push","clear","catch","$message","resetParam","add","$nextTick","resetForm","updatePark","row","function","deletePark","get","showClose","handleClose","done","_","save","$refs","validate","valid","uid","doUpdate","doSave","resetFields","beforeMount"],"sources":["src/components/teaPantation/teaPantationParkMain.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-bottom: 5px\">\r\n      <el-input v-model=\"pid\"\r\n                placeholder=\"请输入园区id\"\r\n                suffix-icon=\"el-icon-search\"\r\n                style=\"width: 200px\"\r\n                @keyup.enter.native=\"loadPost\"></el-input>\r\n      <el-button type=\"primary\"\r\n                 style=\"margin-left: 5px\"\r\n                 @click=\"loadPost\">查询</el-button>\r\n      <el-button type=\"success\"\r\n                 @click=\"resetParam\">重置</el-button>\r\n      <el-button type=\"primary\"\r\n                 style=\"margin-left: 5px\"\r\n                 @click=\"add\">新增</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\"\r\n              :header-cell-style=\"{backgroundColor:'rgba(156,180,184,0.5)'}\"\r\n              :border=\"true\">\r\n      <el-table-column prop=\"pid\" label=\"pId\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"pname\" label=\"园区名称\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"area\" label=\"园区面积\" width=\"160\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"aid\" label=\"园区管理员ID\" width=\"80\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"function\" label=\"园区职能\" width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"operate\" label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button size=\"small\"\r\n                     type=\"success\"\r\n                     @click=\"updatePark(scope.row)\">修改</el-button>\r\n          <el-popconfirm title=\"确定删除吗？\"\r\n                         style=\"margin-left: 5px\"\r\n                         @confirm=\"deletePark(scope.row)\">\r\n            <el-button slot=\"reference\"\r\n                       size=\"small\"\r\n                       type=\"danger\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!--分页组件-->\r\n    <el-pagination\r\n        style=\"margin-top: 15px\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"pageNum\"\r\n        :page-sizes=\"[ 5, 10, 15,20]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n    </el-pagination>\r\n    <!--对话框 centerDialogVisibley用来控制对话框是否出现（true表示出现）-->\r\n    <!--超级管理员对话框-->\r\n    <el-dialog\r\n        title=\"提示\"\r\n        :visible.sync=\"centerDialogVisible\"\r\n        width=\"30%\"\r\n        center\r\n        :before-close=\"handleClose\">\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item prop=\"pname\" label=\"名称\">\r\n          <el-input v-model=\"form.pname\" style=\"width: 80%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"area\" label=\"面积\">\r\n          <el-input v-model=\"form.area\" style=\"width: 80%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"aid\" label=\"管理员id\">\r\n          <el-input v-model=\"form.aid\" style=\"width: 80%\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"function\" label=\"园区职能\">\r\n          <el-input v-model=\"form.pFunction\" style=\"width: 80%\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n    <el-button @click=\"centerDialogVisible = false\">取 消</el-button>\r\n    <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n  </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"teaPantationParkMain\",\r\n  data() {\r\n    return {\r\n      user : JSON.parse(sessionStorage.getItem(\"CurUser\")),\r\n      tableData:[],\r\n      pageNum:1,\r\n      pageSize:10,\r\n      total:0,\r\n      pid:'',\r\n      centerDialogVisible:false,\r\n      form:{\r\n        pname:'',\r\n        area:'',\r\n        aid:'',\r\n        pFunction:''\r\n      },\r\n      rules: {\r\n        pname: [\r\n           { required: true, message: '请输入名称', trigger: 'blur'},\r\n        ],\r\n        area: [\r\n          { required: true, message: '请输入面积', trigger: 'blur' },\r\n        ],\r\n        aid:[\r\n          { required: true, message: '请输入管理员ID', trigger: 'blur'}\r\n        ],\r\n        pFunction: [\r\n          { required: true, message: '请输入园区职能', trigger: 'blur'}\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    handleSizeChange(val) {\r\n      console.log(`每页 ${val} 条`);\r\n      this.pageNum = 1;\r\n      this.pageSize=val;\r\n      this.loadPost();\r\n    },\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.pageNum = val;\r\n      this.loadPost();\r\n    },\r\n    loadPost(){\r\n      if(this.user != null && (this.user.role == '0'||this.user.role == '1')){\r\n        this.$axios.post(this.$httpUrl+'/teaPark/listAllPage',\r\n            {\r\n              pageNum:this.pageNum,\r\n              pageSize:this.pageSize,\r\n              param:{\r\n                pid:this.pid\r\n              }\r\n            }\r\n        ).then(res=>res.data).then(res=>{\r\n          console.log(res);\r\n          if(res.code==200){\r\n            this.tableData = res.data;\r\n            this.total = res.total;\r\n          }else {\r\n            alert(\"数据获取失败\");\r\n          }\r\n        });\r\n      }else {\r\n        this.$confirm('未登入请登录后再进入？','提示',{\r\n          confirmButtonText:'确定去登入',//确认按钮文字显示\r\n          type:'warning',\r\n          center:true,//文字居中\r\n          showCancelButton:false,//不显示取消按钮\r\n          closeOnClickModal:false,//是否可以点击空白处关闭弹窗\r\n        }).then(()=>{\r\n          this.$router.push('/');\r\n          sessionStorage.clear();\r\n        }).catch(()=>{\r\n          this.$message({\r\n            type:\"info\",\r\n            message:\"大哥哥(姐姐)快去登录吧！！\"\r\n          });\r\n        });\r\n      }\r\n    },\r\n    resetParam(){\r\n      this.pid = '';\r\n    },\r\n    add(){\r\n        this.centerDialogVisible = true;\r\n        this.$nextTick(()=>{\r\n          this.resetForm();\r\n        });\r\n    },\r\n    updatePark(row){\r\n      console.log(row);\r\n      this.centerDialogVisible = true;\r\n      this.$nextTick(()=>{\r\n        //赋值到表单\r\n        this.form.pid = row.pid;\r\n        this.form.pname = row.pname;\r\n        this.form.aid = row.aid;\r\n        this.form.area = row.area+'';\r\n        this.form.pFunction = row.function;\r\n      });\r\n    },\r\n    deletePark(row){\r\n      console.log(row);\r\n        this.$axios.get(this.$httpUrl+'/teaPark/deletePark?pid='+row.pid)\r\n            .then(res=>res.data).then(res=>{\r\n          console.log(res);\r\n          if(res.code==200){\r\n            this.$message({\r\n              showClose: true,\r\n              message: '数据删除成功',\r\n              type: 'success'\r\n            });\r\n            this.loadPost();\r\n          }else {\r\n            this.$message({\r\n              showClose: true,\r\n              message: '数据删除失败',\r\n              type: 'error'\r\n            });\r\n          }\r\n        });\r\n    },\r\n    handleClose(done) {\r\n      this.$confirm('确认关闭？')\r\n          .then(_ => {\r\n            done();\r\n          })\r\n          .catch(_ => {});\r\n    },\r\n    save(){\r\n      this.$refs.form.validate((valid) => {\r\n        if (valid) {\r\n          if (this.form.uid) {\r\n            this.doUpdate();\r\n          } else {\r\n            this.doSave();\r\n          }\r\n        }else {\r\n          console.log('Error!!!')\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    doSave(){\r\n      this.$axios.post(this.$httpUrl+'/teaPark/addPark', this.form)\r\n          .then(res=>res.data).then(res=>{\r\n        console.log(res);\r\n        if(res.code==200){\r\n          this.$message({\r\n            showClose: true,\r\n            message: '数据添加成功',\r\n            type: 'success'\r\n          });\r\n          this.centerDialogVisible = false;\r\n          this.loadPost();\r\n          this.resetForm();\r\n        }else {\r\n          this.$message({\r\n            showClose: true,\r\n            message: '数据添加失败',\r\n            type: 'error'\r\n          });\r\n        }\r\n      });\r\n    },\r\n    doUpdate(){\r\n      this.$axios.post(this.$httpUrl+'/teaPark/updatePark', this.form)\r\n          .then(res=>res.data).then(res=>{\r\n        console.log(res);\r\n        if(res.code==200){\r\n          this.$message({\r\n            showClose: true,\r\n            message: '数据修改成功',\r\n            type: 'success'\r\n          });\r\n          this.centerDialogVisible = false;\r\n          this.loadPost();\r\n          this.resetForm();\r\n        }else {\r\n          this.$message({\r\n            showClose: true,\r\n            message: '数据修改失败',\r\n            type: 'error'\r\n          });\r\n        }\r\n      });\r\n    },\r\n    resetForm() {\r\n      this.$refs.form.resetFields();\r\n    }\r\n  },\r\n  beforeMount() {\r\n    this.loadPost();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AAuFA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,OAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,GAAA;MACAC,mBAAA;MACAC,IAAA;QACAC,KAAA;QACAC,IAAA;QACAC,GAAA;QACAC,SAAA;MACA;MACAC,KAAA;QACAJ,KAAA,GACA;UAAAK,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAN,IAAA,GACA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAL,GAAA,GACA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAJ,SAAA,GACA;UAAAE,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,OAAA;IACAC,iBAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,OAAAF,GAAA;MACA,KAAAhB,OAAA;MACA,KAAAC,QAAA,GAAAe,GAAA;MACA,KAAAG,QAAA;IACA;IACAC,oBAAAJ,GAAA;MACAC,OAAA,CAAAC,GAAA,SAAAF,GAAA;MACA,KAAAhB,OAAA,GAAAgB,GAAA;MACA,KAAAG,QAAA;IACA;IACAA,SAAA;MACA,SAAAzB,IAAA,kBAAAA,IAAA,CAAA2B,IAAA,gBAAA3B,IAAA