{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"teaProcessMain\",\n  data() {\n    return {\n      user: JSON.parse(sessionStorage.getItem(\"CurUser\")),\n      makekind: '乌龙茶',\n      makekinds: [{\n        value: '乌龙茶',\n        label: '乌龙茶'\n      }, {\n        value: '白茶',\n        label: '白茶'\n      }],\n      speed: null,\n      ready: '',\n      workidList: [],\n      active: 0,\n      processList: [],\n      customColors: [{\n        color: '#f56c6c',\n        percentage: 20\n      }, {\n        color: '#e6a23c',\n        percentage: 40\n      }, {\n        color: '#5cb87a',\n        percentage: 60\n      }, {\n        color: '#1989fa',\n        percentage: 80\n      }, {\n        color: '#6f7ad3',\n        percentage: 100\n      }]\n    };\n  },\n  methods: {\n    resetParam() {\n      this.makekind = '';\n      this.speed = '';\n    },\n    getProcessList() {\n      this.$axios.get(this.$httpUrl + '/teaLeaf/getProcessList?makekind=' + this.makekind).then(res => res.data).then(res => {\n        if (res.code == 200) {\n          this.processList = res.data;\n          for (let item of this.processList) {\n            this.workidList.push(item.workid);\n          }\n          this.active = 0;\n          this.$message({\n            showClose: true,\n            message: '成功',\n            type: 'success'\n          });\n        } else {\n          this.$message({\n            showClose: true,\n            message: '失败',\n            type: 'error'\n          });\n        }\n      });\n    },\n    next() {\n      this.$axios.post(this.$httpUrl + '/teaLeaf/updateWorkSpeed', {\n        readyweight: this.ready,\n        workid: this.workidList[this.active]\n      }).then(res => res.data).then(res => {\n        console.log(res);\n        if (res.code == 200) {\n          if (this.processList[this.active].speed === 100) {\n            this.processList[this.active].speed = 0;\n            if (this.active++ >= this.processList.length) {\n              this.active = 0;\n            }\n          } else {\n            this.processList[this.active].speed = res.data.speed;\n          }\n          this.ready = null;\n        } else {\n          alert(\"数据获取失败\");\n        }\n      });\n    }\n  },\n  beforeMount() {\n    this.getProcessList();\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","sessionStorage","getItem","makekind","makekinds","value","label","speed","ready","workidList","active","processList","customColors","color","percentage","methods","resetParam","getProcessList","$axios","get","$httpUrl","then","res","code","item","push","workid","$message","showClose","message","type","next","post","readyweight","console","log","length","alert","beforeMount"],"sources":["src/components/teaLeaf/teaProcessMain.vue"],"sourcesContent":["<template>\r\n  <div class=\"body\">\r\n    <div class=\"heard\" style=\"margin-bottom: 20px;\">\r\n      <el-select v-model=\"makekind\"\r\n                  filterable placeholder=\"请选择茶叶种类\"\r\n                  style=\"margin-left: 5px\">\r\n      <el-option\r\n          v-for=\"item in makekinds\"\r\n          :key=\"item.value\"\r\n          :label=\"item.label\"\r\n          :value=\"item.value\">\r\n      </el-option>\r\n    </el-select>\r\n      <el-button\r\n          type=\"primary\"\r\n          @click=\"getProcessList\"\r\n          style=\"margin-left: 5px;\">查询</el-button>\r\n      <el-button type=\"success\"\r\n                 @click=\"resetParam\">重置</el-button>\r\n      <el-input v-model=\"ready\"\r\n                placeholder=\"请输入完成的面积或重量\"\r\n                style=\"margin-left: 5px;width: 200px\"></el-input>\r\n      <el-button type=\"primary\"\r\n                 style=\"margin-left: 5px;\"\r\n                 @click=\"next\">下一步</el-button>\r\n    </div>\r\n    <div class=\"stepBody\">\r\n      <el-steps :active=\"active\"\r\n                align-center\r\n                finish-status=\"success\"\r\n                direction=\"vertical\">\r\n        <el-step v-for=\"(value,key) in this.processList\">\r\n          <template slot=\"description\">\r\n            <div>\r\n              {{value.workname}}\r\n            </div>\r\n            <div>\r\n              <el-progress :percentage=\"value.speed\" :color=\"customColors\"></el-progress>\r\n            </div>\r\n          </template>\r\n        </el-step>\r\n      </el-steps>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"teaProcessMain\",\r\n  data() {\r\n    return {\r\n      user : JSON.parse(sessionStorage.getItem(\"CurUser\")),\r\n      makekind: '乌龙茶',\r\n      makekinds:[\r\n        {\r\n          value: '乌龙茶',\r\n          label: '乌龙茶'\r\n        },\r\n        {\r\n          value: '白茶',\r\n          label: '白茶'\r\n        }\r\n      ],\r\n      speed: null,\r\n      ready:'',\r\n      workidList: [],\r\n      active: 0,\r\n      processList:[],\r\n      customColors: [\r\n        {color: '#f56c6c', percentage: 20},\r\n        {color: '#e6a23c', percentage: 40},\r\n        {color: '#5cb87a', percentage: 60},\r\n        {color: '#1989fa', percentage: 80},\r\n        {color: '#6f7ad3', percentage: 100}\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    resetParam() {\r\n      this.makekind = '';\r\n      this.speed = '';\r\n    },\r\n    getProcessList() {\r\n      this.$axios.get(this.$httpUrl+'/teaLeaf/getProcessList?makekind='+this.makekind)\r\n          .then(res => res.data).then(res => {\r\n        if (res.code == 200) {\r\n          this.processList = res.data;\r\n          for(let item of this.processList) {\r\n            this.workidList.push(item.workid);\r\n          }\r\n          this.active = 0;\r\n          this.$message({\r\n            showClose: true,\r\n            message: '成功',\r\n            type: 'success'\r\n          });\r\n        } else {\r\n          this.$message({\r\n            showClose: true,\r\n            message: '失败',\r\n            type: 'error'\r\n          });\r\n        }\r\n      });\r\n    },\r\n    next() {\r\n      this.$axios.post(this.$httpUrl+'/teaLeaf/updateWorkSpeed',\r\n          {\r\n            readyweight:this.ready,\r\n            workid:this.workidList[this.active]\r\n          }\r\n      ).then(res=>res.data).then(res=>{\r\n        console.log(res);\r\n        if(res.code==200){\r\n          if(this.processList[this.active].speed === 100){\r\n            this.processList[this.active].speed = 0;\r\n            if(this.active++ >= this.processList.length){\r\n              this.active = 0;\r\n            }\r\n          } else {\r\n            this.processList[this.active].speed = res.data.speed;\r\n          }\r\n          this.ready = null;\r\n        }else {\r\n          alert(\"数据获取失败\");\r\n        }\r\n      });\r\n    }\r\n  },\r\n  beforeMount() {\r\n    this.getProcessList();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .stepBody {\r\n    width: 90%;\r\n    height: 90%;\r\n    margin-left: 3%;\r\n    padding: 2%;\r\n    border-radius: 15px;\r\n    border: 1px gray solid;\r\n    box-shadow: 0 3px 6px rgba(0, 0, 0, .12), 0 2px 6px rgba(0, 0, 0, .04);\r\n  }\r\n</style>"],"mappings":";AA+CA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,OAAA;MACAC,QAAA;MACAC,SAAA,GACA;QACAC,KAAA;QACAC,KAAA;MACA,GACA;QACAD,KAAA;QACAC,KAAA;MACA,EACA;MACAC,KAAA;MACAC,KAAA;MACAC,UAAA;MACAC,MAAA;MACAC,WAAA;MACAC,YAAA,GACA;QAAAC,KAAA;QAAAC,UAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,UAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,UAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,UAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,UAAA;MAAA;IAEA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAb,QAAA;MACA,KAAAI,KAAA;IACA;IACAU,eAAA;MACA,KAAAC,MAAA,CAAAC,GAAA,MAAAC,QAAA,8CAAAjB,QAAA,EACAkB,IAAA,CAAAC,GAAA,IAAAA,GAAA,CAAAzB,IAAA,EAAAwB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAZ,WAAA,GAAAW,GAAA,CAAAzB,IAAA;UACA,SAAA2B,IAAA,SAAAb,WAAA;YACA,KAAAF,UAAA,CAAAgB,IAAA,CAAAD,IAAA,CAAAE,MAAA;UACA;UACA,KAAAhB,MAAA;UACA,KAAAiB,QAAA;YACAC