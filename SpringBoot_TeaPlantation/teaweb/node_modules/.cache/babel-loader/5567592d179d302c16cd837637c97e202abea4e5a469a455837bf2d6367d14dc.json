      type: 'success'\n          });\n          //跳转到提交页面\n          this.$router.push('/approvalMain');\n        } else {\n          this.$message({\n            showClose: true,\n            message: '数据添加失败',\n            type: 'error'\n          });\n        }\n      });\n    },\n    save() {\n      this.$axios.post(this.$httpUrl + '/equipment/addRepository', this.form).then(res => res.data).then(res => {\n        if (res.code == 200) {\n          this.$message({\n            showClose: true,\n            message: '数据添加成功',\n            type: 'success'\n          });\n          this.centerDialogVisible = false;\n          this.loadPost();\n          this.resetForm();\n        } else {\n          this.$message({\n            showClose: true,\n            message: '数据添加失败',\n            type: 'error'\n          });\n        }\n      });\n    }\n  },\n  beforeMount() {\n    this.loadPost();\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","sessionStorage","getItem","tableData","rid","eid","pageNum","pageSize","total","centerDialogVisible","actionForm","actorid","power","action","actiontime","status","form","rname","stock","pushnumber","popnumber","methods","handleSizeChange","val","console","log","loadPost","handleCurrentChange","handleClose","done","$confirm","then","_","catch","resetActionForm","$axios","post","$httpUrl","param","res","code","alert","confirmButtonText","type","center","showCancelButton","closeOnClickModal","$router","push","clear","$message","message","resetParam","resetForm","add","$nextTick","approvalPush","row","uid","role","Date","toApproval","approvalPop","showClose","save","beforeMount"],"sources":["src/components/equipment/repositoryManageMain.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-bottom: 5px\">\r\n      <el-input v-model=\"rid\"\r\n                placeholder=\"请输入仓库id\"\r\n                suffix-icon=\"el-icon-search\"\r\n                style=\"width: 200px\"\r\n              