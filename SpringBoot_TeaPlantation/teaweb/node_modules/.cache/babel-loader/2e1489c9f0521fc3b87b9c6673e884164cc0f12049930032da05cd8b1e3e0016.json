is.$message({\r\n          type:\"success\",\r\n          message:\"退出登入成功\"\r\n        });\r\n        this.$router.push('/');\r\n        sessionStorage.clear();\r\n      }).catch(()=>{\r\n        if (this.user.usex == '1'){\r\n          this.$message({\r\n            type:\"info\",\r\n            message:\"取消退出，大哥哥欢迎回来\"\r\n          });\r\n        }\r\n        else if (this.user.usex == '0') {\r\n          this.$message({\r\n            type:\"info\",\r\n            message:\"取消退出，大姐姐欢迎回来\"\r\n          });\r\n        }\r\n      });\r\n    },\r\n    collapse(){\r\n      this.$emit('doCollapse');\r\n    },\r\n    toSubmit(){\r\n      console.log(this.user[\"role\"]);\r\n      if(this.user.role == '0' || this.user.role == '1'){\r\n        alert(\"审批\")\r\n      }\r\n      else{\r\n        alert(\"提交\")\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.$router.push('/indexMain');\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AAiCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,KAAA;IACAC,SAAA,EAAAC;EACA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACAC,SAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QAAA;QACAC,gBAAA;QAAA;QACAC,IAAA;QACAC,MAAA;QAAA;QACAC,gBAAA;QAAA;QACAC,SAAA;QAAA;QACAC,iBAAA;MACA,GAAAC,IAAA;QACA,KAAAC,QAAA;UACAN,IAAA;UACAO,OAAA;QACA;QACA,KAAAb,OAAA,CAAAC,IAAA;QACAT,cAAA,CAAAsB,KAAA;MACA,GAAAC,KAAA;QACA,SAAA1B,IAAA,CAAA2B,IAAA;UACA,KAAAJ,QAAA;YACAN,IAAA;YACAO,OAAA;UACA;QACA,OACA,SAAAxB,IAAA,CAAA2B,IAAA;UACA,KAAAJ,QAAA;YACAN,IAAA;YACAO,OAAA;UACA;QACA;MACA;IACA;IACAI,SAAA;MACA,KAAAC,KAAA;IACA;IACAC,SAAA;MACAC,OAAA,CAAAC,GAAA,MAAAhC,IAAA;MACA,SAAAA,IAAA,CAAAiC,IAAA,gBAAAjC,IAAA,CAAAiC,IAAA;QACAC,KAAA;MACA,OACA;QACAA,KAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAxB,OAAA,CAAAC,IAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}