oCollapse');\r\n    },\r\n    toSubmit(){\r\n      if(this.user.role == '0' || this.user.role == '1'){\r\n        alert(\"审批\")\r\n      }\r\n      else{\r\n        alert(\"提交\")\r\n      }\r\n    },\r\n    getDownList(){\r\n      if(this.user.role == '0' || this.user.role == '1'){\r\n        this.downList = [\r\n          {\r\n            name:'个人中心',\r\n            fun:'toUser',\r\n            icon:'el-icon-setting'\r\n          },\r\n          {\r\n            name:'审批',\r\n            fun:'toApprove',\r\n            icon:'el-icon-bottom'\r\n          },\r\n          {\r\n            name:'退出',\r\n            fun:'loginOut',\r\n            icon:'el-icon-switch-button'\r\n          }\r\n        ];\r\n      }\r\n      else{\r\n        this.downList = [\r\n          {\r\n            name:'个人中心',\r\n            fun:'toUser',\r\n            icon:'el-icon-setting'\r\n          },\r\n          {\r\n            name:'提交',\r\n            fun:'toSubmit',\r\n            icon:'el-icon-top'\r\n          },\r\n          {\r\n            name:'退出',\r\n            fun:'loginOut',\r\n            icon:'el-icon-switch-button'\r\n          }\r\n        ];\r\n      }\r\n    }\r\n  },\r\n  beforeMount() {\r\n    this.getDownList();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AA6CA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,KAAA;IACAC,SAAA,EAAAC;EACA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACAC,SAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QAAA;QACAC,gBAAA;QAAA;QACAC,IAAA;QACAC,MAAA;QAAA;QACAC,gBAAA;QAAA;QACAC,SAAA;QAAA;QACAC,iBAAA;MACA,GAAAC,IAAA;QACA,KAAAC,QAAA;UACAN,IAAA;UACAO,OAAA;QACA;QACA,KAAAb,OAAA,CAAAC,IAAA;QACAT,cAAA,CAAAsB,KAAA;MACA,GAAAC,KAAA;QACA,SAAA1B,IAAA,CAAA2B,IAAA;UACA,KAAAJ,QAAA;YACAN,IAAA;YACAO,OAAA;UACA;QACA,OACA,SAAAxB,IAAA,CAAA2B,IAAA;UACA,KAAAJ,QAAA;YACAN,IAAA;YACAO,OAAA;UACA;QACA;MACA;IACA;IACAI,SAAA;MACA,KAAAC,KAAA;IACA;IACAC,SAAA;MACA,SAAA9B,IAAA,CAAA+B,IAAA